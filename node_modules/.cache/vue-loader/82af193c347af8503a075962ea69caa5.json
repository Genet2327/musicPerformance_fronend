      ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"lesson.description\"\r\n          id=\"description\"\r\n          :counter=\"50\"\r\n          label=\"Description\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-btn\r\n          :disabled=\"!valid\"\r\n          color=\"success\"\r\n          class=\"mr-4\"\r\n          @click=\"saveLesson()\"\r\n        >\r\n          Save\r\n        </v-btn>\r\n\r\n        <v-btn color=\"error\" class=\"mr-4\" @click=\"cancel()\"> Cancel </v-btn>\r\n      </v-form>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport LessonServices from \"../services/lessonServices\";\r\n\r\nexport default {\r\n  name: \"edit-lesson\",\r\n  props: [\"tutorialId\", \"lessonId\"],\r\n  data() {\r\n    return {\r\n      valid: false,\r\n      lesson: {},\r\n      message: \"Enter data and click save\",\r\n    };\r\n  },\r\n  methods: {\r\n    retrieveLesson() {\r\n      LessonServices.getLesson(this.tutorialId, this.lessonId)\r\n        .then((response) => {\r\n          this.lesson = response.data;\r\n        })\r\n        .catch((e) => {\r\n          this.message = e.response.data.message;\r\n        });\r\n    },\r\n    saveLesson() {\r\n      var data = {\r\n        title: this.lesson.title,\r\n        description: this.lesson.description,\r\n        tutorialId: this.lesson.tutorialId,\r\n      };\r\n      LessonServices.updateLesson(this.lesson.tutorialId, this.lesson.id, data)\r\n        .then((response) => {\r\n          this.lesson.id = response.data.id;\r\n\r\n          this.$router.push({\r\n            name: \"view\",\r\n            params: { id: this.lesson.tutorialId },\r\n          });\r\n        })\r\n        .catch((e) => {\r\n          this.message = e.response.data.message;\r\n        });\r\n    },\r\n    cancel() {\r\n      this.$router.push({\r\n        name: \"view\",\r\n        params: { id: this.lesson.tutorialId },\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.retrieveLesson();\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n"]}]}