1\"\r\n          :close-on-content-click=\"false\"\r\n          :return-value.sync=\"event.date\"\r\n          transition=\"scale-transition\"\r\n          offset-y\r\n          min-width=\"290px\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-text-field\r\n              v-model=\"event.date\"\r\n              label=\"Date\"\r\n              prepend-icon=\"event\"\r\n              readonly\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            ></v-text-field>\r\n          </template>\r\n          <v-date-picker v-model=\"event.date\" no-title scrollable>\r\n            <v-spacer></v-spacer>\r\n            <v-btn text color=\"primary\" @click=\"menu1 = false\">Cancel</v-btn>\r\n            <v-btn text color=\"primary\" @click=\"$refs.menu1.save(event.date)\">OK</v-btn>\r\n          </v-date-picker>\r\n        </v-menu>\r\n        <v-text-field\r\n          id=\"room\"\r\n          v-model=\"event.room\"\r\n          :counter=\"50\"\r\n          label=\"Room\"\r\n          required\r\n        ></v-text-field>\r\n        <v-menu\r\n          ref=\"menu\"\r\n          v-model=\"menu2\"\r\n          :close-on-content-click=\"false\"\r\n          :nudge-right=\"40\"\r\n          :return-value.sync=\"event.startTime\"\r\n          transition=\"scale-transition\"\r\n          offset-y\r\n          max-width=\"290px\"\r\n          min-width=\"290px\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-text-field\r\n              v-model=\"event.startTime\"\r\n              label=\"Start Time\"\r\n              prepend-icon=\"mdi-clock-time-four-outline\"\r\n              readonly\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            ></v-text-field>\r\n          </template>\r\n          <v-time-picker\r\n            v-if=\"menu2\"\r\n            v-model=\"event.startTime\"\r\n            full-width\r\n            @click:minute=\"$refs.menu.save(event.startTime)\"\r\n          ></v-time-picker>\r\n        </v-menu>\r\n        <v-menu\r\n          ref=\"menu\"\r\n          v-model=\"menu3\"\r\n          :close-on-content-click=\"false\"\r\n          :nudge-right=\"40\"\r\n          :return-value.sync=\"event.endTime\"\r\n          transition=\"scale-transition\"\r\n          offset-y\r\n          max-width=\"290px\"\r\n          min-width=\"290px\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-text-field\r\n              v-model=\"event.endTime\"\r\n              label=\"End Time\"\r\n              prepend-icon=\"mdi-clock-time-four-outline\"\r\n              readonly\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            ></v-text-field>\r\n          </template>\r\n          <v-time-picker\r\n            v-if=\"menu3\"\r\n            v-model=\"event.endTime\"\r\n            full-width\r\n            @click:minute=\"$refs.menu.save(event.endTime)\"\r\n          ></v-time-picker>\r\n        </v-menu>\r\n\r\n        <v-btn\r\n          :disabled=\"!valid\"\r\n          color=\"success\"\r\n          class=\"mr-4\"\r\n          @click=\"saveEvent()\"\r\n        >\r\n          Save\r\n        </v-btn>\r\n\r\n        <v-btn color=\"error\" class=\"mr-4\" @click=\"cancel()\"> Cancel </v-btn>\r\n      </v-form>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport EventServices from \"../../services/Event/services\";\r\n\r\nexport default {\r\n  name: \"EditEvent\",\r\n  props: {\r\n    eventSessionId: {\r\n      type: [Number, String],\r\n      default: 0,\r\n    },\r\n    eventId: {\r\n      type: [Number, String],\r\n      default: 0,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      valid: true,\r\n      menu2: false,\r\n      menu1: false,\r\n      menu3: false,\r\n      event: {},\r\n      message: \"Enter data and click save\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.retrieveEvent();\r\n  },\r\n  methods: {\r\n    retrieveEvent() {\r\n      EventServices.getEvent(this.eventSessionId, this.eventId)\r\n        .then((response) => {\r\n          this.event = response.data;\r\n        })\r\n        .catch((e) => {\r\n          this.message = e.response.data.message;\r\n        });\r\n    },\r\n    saveEvent() {\r\n      var data = {\r\n        date: this.event.date,\r\n        room: this.event.room,\r\n        startTime: this.event.startTime,\r\n        endTime: this.event.endTime,\r\n      };\r\n      EventServices.updateEvent(this.event.eventSessionId, this.event.id, data)\r\n        .then((response) => {\r\n          this.event.id = response.data.id;\r\n\r\n          this.$router.push({\r\n            name: \"ViewEventSession\",\r\n            params: { id: this.event.eventSessionId },\r\n          });\r\n        })\r\n        .catch((e) => {\r\n          this.message = e.response.data.message;\r\n        });\r\n    },\r\n    cancel() {\r\n      this.$router.push({\r\n        name: \"ViewEventSession\",\r\n        params: { id: this.event.eventSessionId },\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n"]}]}