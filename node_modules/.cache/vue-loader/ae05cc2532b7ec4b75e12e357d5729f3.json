h\"\r\n            single-line\r\n            hide-details\r\n          >\r\n          </v-text-field>\r\n        </v-card-title>\r\n        <v-card-text>\r\n          <b>{{ message }}</b>\r\n        </v-card-text>\r\n        <v-data-table\r\n          :headers=\"headers\"\r\n          :search=\"search\"\r\n          :items=\"roles\"\r\n          :items-per-page=\"50\"\r\n        >\r\n          <template v-slot:[`item.actions`]=\"{ item }\">\r\n            <div>\r\n              <!-- <v-icon small class=\"mx-4\" @click=\"ADDRoleForUser(item)\">\r\n                    mdi-pencil\r\n                  </v-icon> -->\r\n\r\n              <v-icon\r\n               \r\n                small\r\n                class=\"mx-4\"\r\n                @click=\"editRole(item)\"\r\n              >\r\n                mdi-pencil\r\n              </v-icon>\r\n              <!-- <v-icon small class=\"mx-4\" @click=\"viewRole(item)\">\r\n                  mdi-format-list-bulleted-type\r\n                </v-icon> -->\r\n              <!-- <v-icon small class=\"mx-4\" @click=\"deleteRole(item)\">\r\n                  mdi-trash-can\r\n                </v-icon> -->\r\n            </div>\r\n          </template>\r\n        </v-data-table>\r\n      </v-card>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RoleServices from \"../../services/Role/services\";\r\nimport Utils from \"@/config/utils.js\";\r\n\r\nexport default {\r\n  name: \"roles-list\",\r\n  data() {\r\n    return {\r\n      search: \"\",\r\n      roles: [],\r\n      currentRole: null,\r\n      currentIndex: -1,\r\n      name: \"\",\r\n      user: {},\r\n      message: \"Search, Edit Roles\",\r\n      headers: [\r\n        { text: \"First Name\", value: \"fName\" },\r\n        { text: \"Last Name\", value: \"lName\" },\r\n        { text: \"Role\", value: \"roles[0].name\" },\r\n        { text: \"Actions\", value: \"actions\", sortable: false },\r\n      ],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.user = Utils.getStore(\"user\");\r\n    this.retrieveRoles();\r\n  },\r\n  methods: {\r\n    editRole(role) {\r\n      this.$router.push({ name: \"EditRole\", params: { id: role.id } });\r\n    },\r\n    viewRole(role) {\r\n      this.$router.push({ name: \"viewRole\", params: { id: role.id } });\r\n    },\r\n\r\n    deleteRole(role) {\r\n      RoleServices.delete(role.id)\r\n        .then(() => {\r\n          this.retrieveRoles();\r\n        })\r\n        .catch((e) => {\r\n          this.message = e.response.data.message;\r\n        });\r\n    },\r\n    retrieveRoles() {\r\n      RoleServices.getAll()\r\n        .then((response) => {\r\n          this.roles = response.data;\r\n        })\r\n        .catch((e) => {\r\n          this.message = e.response.data.message;\r\n        });\r\n    },\r\n    refreshList() {\r\n      this.retrieveRoles();\r\n      this.currentRole = null;\r\n      this.currentIndex = -1;\r\n    },\r\n    setActiveRole(role, index) {\r\n      this.currentRole = role;\r\n      this.currentIndex = role ? index : -1;\r\n    },\r\n    removeAllRoles() {\r\n      RoleServices.deleteAll()\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.refreshList();\r\n        })\r\n        .catch((e) => {\r\n          this.message = e.response.data.message;\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}