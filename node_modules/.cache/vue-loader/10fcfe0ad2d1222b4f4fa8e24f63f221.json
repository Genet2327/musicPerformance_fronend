  </v-text-field>\r\n        <v-text-field\r\n          v-model=\"composer.lastName\"\r\n          id=\"lastName\"\r\n          :counter=\"50\"\r\n          label=\" Last Name\"\r\n          required\r\n        >\r\n        </v-text-field>\r\n        <v-text-field\r\n          v-model=\"composer.nationality\"\r\n          id=\"nationality\"\r\n          :counter=\"50\"\r\n          label=\" Nationality\"\r\n          required\r\n        >\r\n        </v-text-field>\r\n        <v-menu\r\n          ref=\"menu\"\r\n          v-model=\"menu\"\r\n          :close-on-content-click=\"false\"\r\n          :return-value.sync=\"date\"\r\n          transition=\"scale-transition\"\r\n          offset-y\r\n          min-width=\"290px\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-text-field\r\n              v-model=\"composer.dateOfBirth\"\r\n              label=\"Date of Birth\"\r\n              prepend-icon=\"event\"\r\n              readonly\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            ></v-text-field>\r\n          </template>\r\n          <v-date-picker v-model=\"composer.dateOfBirth\" no-title scrollable>\r\n            <v-spacer></v-spacer>\r\n            <v-btn text color=\"primary\" @click=\"menu = false\">Cancel</v-btn>\r\n            <v-btn text color=\"primary\" @click=\"$refs.menu.save(composer.dateOfBirth)\">OK</v-btn>\r\n          </v-date-picker>\r\n        </v-menu>\r\n        <v-menu\r\n          ref=\"menu1\"\r\n          v-model=\"menu1\"\r\n          :close-on-content-click=\"false\"\r\n          :return-value.sync=\"date\"\r\n          transition=\"scale-transition\"\r\n          offset-y\r\n          min-width=\"290px\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-text-field\r\n              v-model=\"composer.dateOfDeath\"\r\n              label=\"Date of Death\"\r\n              prepend-icon=\"event\"\r\n              readonly\r\n    