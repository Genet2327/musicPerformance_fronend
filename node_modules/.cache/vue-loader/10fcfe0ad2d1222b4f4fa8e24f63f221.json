  this.$router.push({ name: \"ComposerList\" });\r\n          })\r\n          .catch((e) => {\r\n            this.message = e.response.data.message;\r\n          });\r\n      },\r\n      cancel() {\r\n        this.$router.push({ name: \"ComposerList\" });\r\n      },\r\n    },\r\n    mounted() {\r\n      this.retrieveComposer ();\r\n    },\r\n  };\r\n  </script>\r\n  <style></style>\r\n  "]}]}