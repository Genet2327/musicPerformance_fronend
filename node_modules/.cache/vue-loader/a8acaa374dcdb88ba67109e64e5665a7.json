{"remainingRequest":"C:\\xampp\\htdocs\\Spring2023\\tutorial-frontend-vue2\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\xampp\\htdocs\\Spring2023\\tutorial-frontend-vue2\\src\\views\\Role\\EditRole.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\xampp\\htdocs\\Spring2023\\tutorial-frontend-vue2\\src\\views\\Role\\EditRole.vue","mtime":1673659269421},{"path":"C:\\xampp\\htdocs\\Spring2023\\tutorial-frontend-vue2\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1673567759000},{"path":"C:\\xampp\\htdocs\\Spring2023\\tutorial-frontend-vue2\\node_modules\\babel-loader\\lib\\index.js","mtime":1673567763338},{"path":"C:\\xampp\\htdocs\\Spring2023\\tutorial-frontend-vue2\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1673567759000},{"path":"C:\\xampp\\htdocs\\Spring2023\\tutorial-frontend-vue2\\node_modules\\vue-loader\\lib\\index.js","mtime":1673567766200}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["EditRole.vue"],"names":[],"mappings":";AAkDA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"EditRole.vue","sourceRoot":"src/views/Role","sourcesContent":["<template>\r\n  <div>\r\n    <v-container>\r\n      <v-toolbar>\r\n        <v-toolbar-title>Role Edit</v-toolbar-title>\r\n        <!-- <v-spacer></v-spacer>\r\n          <v-toolbar-title>{{this.message}}</v-toolbar-title> -->\r\n      </v-toolbar>\r\n      <br />\r\n      <h4>{{ message }}</h4>\r\n      <br />\r\n      <v-form ref=\"form\" v-model=\"valid\" lazy validation>\r\n        <v-text-field\r\n          v-model=\"role.fName\"\r\n          id=\"name\"\r\n          :counter=\"50\"\r\n          label=\"First Name\"\r\n          required\r\n        >\r\n        </v-text-field>\r\n        <v-text-field\r\n          v-model=\"role.lName\"\r\n          id=\"name\"\r\n          :counter=\"50\"\r\n          label=\"Last Name\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-select\r\n          v-model=\"roleId\"\r\n          :items=\"items\"\r\n          item-text=\"Select Role\"\r\n        ></v-select>\r\n\r\n        <v-btn\r\n          :disabled=\"!valid\"\r\n          color=\"success\"\r\n          class=\"mr-4\"\r\n          @click=\"updateRole()\"\r\n        >\r\n          Save\r\n        </v-btn>\r\n\r\n        <v-btn color=\"error\" class=\"mr-4\" @click=\"cancel()\"> Cancel </v-btn>\r\n      </v-form>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RoleServices from \"../services/roleServices\";\r\n\r\nexport default {\r\n  name: \"edit-role\",\r\n  props: [\"id\"],\r\n  data() {\r\n    return {\r\n      valid: false,\r\n      items: [\"Stu\", \"Admin\", \"Adv\"],\r\n      role: {},\r\n      roleId: null,\r\n      message: \"Enter data and click save\",\r\n    };\r\n  },\r\n  methods: {\r\n    retrieveRole() {\r\n      RoleServices.get(this.id)\r\n        .then((response) => {\r\n          this.role = response.data;\r\n          this.roleId = this.role.role;\r\n        })\r\n        .catch((e) => {\r\n          this.message = e.response.data.message;\r\n        });\r\n    },\r\n\r\n    updateRole() {\r\n      var data = {\r\n        fName: this.role.fName,\r\n        lName: this.role.lName,\r\n        role: this.roleId,\r\n      };\r\n      RoleServices.update(this.id, data)\r\n        .then((response) => {\r\n          this.role.id = response.data.id;\r\n          this.$router.push({ name: \"RoleList\" });\r\n        })\r\n        .catch((e) => {\r\n          this.message = e.response.data.message;\r\n        });\r\n    },\r\n    cancel() {\r\n      this.$router.push({ name: \"RoleList\" });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.retrieveRole();\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n"]}]}