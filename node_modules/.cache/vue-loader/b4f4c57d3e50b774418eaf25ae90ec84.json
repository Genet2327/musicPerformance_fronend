           v-model=\"role.lName\"\r\n            id=\"name\"\r\n            :counter=\"50\"\r\n            label=\"Last Name\"\r\n            required\r\n          ></v-text-field>\r\n  \r\n          <v-select\r\n            v-model=\"roleId\"\r\n            :items=\"items\"\r\n            item-text=\"Select Role\"\r\n          ></v-select>\r\n  \r\n          <v-btn\r\n            :disabled=\"!valid\"\r\n            color=\"success\"\r\n            class=\"mr-4\"\r\n            @click=\"updateRole()\"\r\n          >\r\n            Save\r\n          </v-btn>\r\n  \r\n          <v-btn color=\"error\" class=\"mr-4\" @click=\"cancel()\"> Cancel </v-btn>\r\n        </v-form>\r\n      </v-container>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import RoleServices from \"../../services/Role/services\";\r\n  \r\n  export default {\r\n    name: \"edit-role\",\r\n    props: [\"id\"],\r\n    data() {\r\n      return {\r\n        valid: false,\r\n        items: [\"Stu\", \"Admin\", \"Adv\"],\r\n        role: {},\r\n        roleId: null,\r\n        message: \"Enter data and click save\",\r\n      };\r\n    },\r\n    methods: {\r\n      retrieveRole() {\r\n        RoleServices.get(this.id)\r\n          .then((response) => {\r\n            this.role = response.data;\r\n            this.roleId = this.role.role;\r\n          })\r\n          .catch((e) => {\r\n            this.message = e.response.data.message;\r\n          });\r\n      },\r\n  \r\n      updateRole() {\r\n        var data = {\r\n          fName: this.role.fName,\r\n          lName: this.role.lName,\r\n          role: this.roleId,\r\n        };\r\n        RoleServices.update(this.id, data)\r\n          .then((response) => {\r\n            this.role.id = response.data.id;\r\n            this.$router.push({ name: \"RoleList\" });\r\n          })\r\n          .catch((e) => {\r\n            this.message = e.response.data.message;\r\n          });\r\n      },\r\n      cancel() {\r\n        this.$router.push({ name: \"RoleList\" });\r\n      },\r\n    },\r\n    mounted() {\r\n      this.retrieveRole();\r\n    },\r\n  };\r\n  </script>\r\n  <style></style>\r\n  "]}]}