"dateOfDeath","cancel","mounted"],"sourceRoot":"src/views/Composer","sources":["Edit.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <v-container>\r\n        <v-toolbar>\r\n          <v-toolbar-title>Composer  Edit</v-toolbar-title>\r\n          <!-- <v-spacer></v-spacer>\r\n            <v-toolbar-title>{{this.message}}</v-toolbar-title> -->\r\n        </v-toolbar>\r\n        <br />\r\n        <h4>{{ message }}</h4>\r\n        <br />\r\n        <v-form ref=\"form\" v-model=\"valid\" lazy validation>\r\n          <v-text-field\r\n          v-model=\"composer.firstName\"\r\n          id=\"firstName\"\r\n          :counter=\"50\"\r\n          label=\" First Name\"\r\n          required\r\n        >\r\n        </v-text-field>\r\n        <v-text-field\r\n          v-model=\"composer.lastName\"\r\n          id=\"lastName\"\r\n          :counter=\"50\"\r\n          label=\" Last Name\"\r\n          required\r\n        >\r\n        </v-text-field>\r\n        <v-text-field\r\n          v-model=\"composer.nationality\"\r\n          id=\"nationality\"\r\n          :counter=\"50\"\r\n          label=\" Nationality\"\r\n          required\r\n        >\r\n        </v-text-field>\r\n        <v-menu\r\n          ref=\"menu\"\r\n          v-model=\"menu\"\r\n          :close-on-content-click=\"false\"\r\n          :return-value.sync=\"date\"\r\n          transition=\"scale-transition\"\r\n          offset-y\r\n          min-width=\"290px\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-text-field\r\n              v-model=\"composer.dateOfBirth\"\r\n              label=\"Date of Birth\"\r\n              prepend-icon=\"event\"\r\n              readonly\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            ></v-text-field>\r\n          </template>\r\n          <v-date-picker v-model=\"composer.dateOfBirth\" no-title scrollable>\r\n            <v-spacer></v-spacer>\r\n            <v-btn text color=\"primary\" @click=\"menu = false\">Cancel</v-btn>\r\n            <v-btn text color=\"primary\" @click=\"$refs.menu.save(composer.dateOfBirth)\">OK</v-btn>\r\n          </v-date-picker>\r\n        </v-menu>\r\n        <v-menu\r\n          ref=\"menu1\"\r\n          v-model=\"menu1\"\r\n          :close-on-content-click=\"false\"\r\n          :return-value.sync=\"date\"\r\n          transition=\"scale-transition\"\r\n          offset-y\r\n          min-width=\"290px\"\r\n        >\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-text-field\r\n              v-model=\"composer.dateOfDeath\"\r\n              label=\"Date of Death\"\r\n              prepend-icon=\"event\"\r\n              readonly\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            ></v-text-field>\r\n          </template>\r\n          <v-date-picker v-model=\"composer.dateOfDeath\" no-title scrollable>\r\n            <v-spacer></v-spacer>\r\n            <v-btn text color=\"primary\" @click=\"menu1 = false\">Cancel</v-btn>\r\n            <v-btn text color=\"primary\" @click=\"$refs.menu1.save(composer.dateOfDeath)\">OK</v-btn>\r\n          </v-date-picker>\r\n        </v-menu>\r\n          \r\n          <v-btn\r\n            :disabled=\"!valid\"\r\n            color=\"success\"\r\n            class=\"mr-4\"\r\n            @click=\"updateComposer ()\"\r\n          >\r\n            Save\r\n          </v-btn>\r\n  \r\n          <v-btn color=\"error\" class=\"mr-4\" @click=\"cancel()\"> Cancel </v-btn>\r\n        </v-form>\r\n      </v-container>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  \r\n  import ComposerServices from \"../../services/Composer/services\";\r\n  \r\n  export default {\r\n    name: \"edit-composer \",\r\n    props: [\"id\"],\r\n    data() {\r\n      return {\r\n        valid: false,\r\n        composer : {},\r\n        message: \"Enter data and click save\",\r\n      };\r\n    },\r\n    methods: {\r\n      retrieveComposer () {\r\n        ComposerServices.get(this.id)\r\n          .then((response) => {\r\n            this.composer  = response.data;\r\n          })\r\n          .catch((e) => {\r\n            this.message = e.response.data.message;\r\n          });\r\n      },\r\n  \r\n      updateComposer () {\r\n        var data = {\r\n        firstName: this.composer.firstName,\r\n        lastName: this.composer.lastName,\r\n        nationality : this.composer.nationality ,\r\n        dateOfBirth: this.composer.dateOfBirth,\r\n        dateOfDeath: this.composer.dateOfDeath,\r\n       \r\n      };\r\n        ComposerServices.update(this.id, data)\r\n          .then((response) => {\r\n            this.composer .id = response.data.id;\r\n            this.$router.push({ name: \"ComposerList\" });\r\n          })\r\n          .catch((e) => {\r\n            this.message = e.response.data.message;\r\n          });\r\n      },\r\n      cancel() {\r\n        this.$router.push({ name: \"ComposerList\" });\r\n      },\r\n    },\r\n    mounted() {\r\n      this.retrieveComposer ();\r\n    },\r\n  };\r\n  </script>\r\n  <style></style>\r\n  "]}]}